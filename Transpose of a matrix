#include <stdio.h>

int main()
{
    int arr[3][3]={1,2,3,4,5,6,7,8,9};
    int i , j;
    int B[3][3];
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            B[i][j]=arr[j][i];
        }
    }
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            printf("%d ",B[i][j]);
        }
        printf("\n");
    }

    return 0;
}

